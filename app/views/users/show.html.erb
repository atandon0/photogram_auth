<div class="page-header">
  <h3>User #<%= @user.id %></h3>
</div>

<% @user.photos.each do |photo| %>
<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Caption</dt>
      <dd><%= photo.caption %></dd>

      <dt>Image</dt>
      <dd><%= photo.image %></dd>
      <% end  %>
    </dl>

  </div>
</div>
